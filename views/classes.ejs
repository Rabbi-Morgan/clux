<body>
    <header class=" d-flex justify-content-center align-items-center header text-white py-4 px-4">
        <div><img class="logo" src="./logo.png" alt=""></div>
    </header>
    <div class="preloader bg-secondary">
        <img src="/preloader.gif" width="60" height="60" alt="spinner">
    </div>
    <div class="container-fluid">
        <div class=" row bg-secondary d-flex justify-content-between text-white py-2">
            <h2 class="col-auto">List of all classes</h2>
            <div class="col-4 text-end"><a href="/logout" class="btn px-4" style="background-color: #DAA425; color:white;"><i class="fa fa-sign-out"></i> Logout</a>
        </div>
        </div>
    </div>
    

    <div  class="container mt-5">

        <div class="row g-3">
            <a href="/addclass" class="btn btn-light text-primary"><i class="fa fa-plus"></i> Create Virtual Class</a>
            <div class="table-responsive">
                <table class="table table-striped table-hover" id="class">
                    
                </table>
            </div>
        </div>
    </div>

    <script src="js/jquery.min.js"></script>
